# change the delivery of the docs to /usr/share/doc.  We only need one
# copy of them so just use the python 2.7 path
<transform file path=usr/(share/doc/rcssmin/.*) -> set action.hash usr/lib/python2.7/vendor-packages/%<1>>
file path=usr/share/doc/rcssmin/CHANGES
file path=usr/share/doc/rcssmin/README.rst
file path=usr/share/doc/rcssmin/apidoc/api-objects.txt
file path=usr/share/doc/rcssmin/apidoc/crarr.png
file path=usr/share/doc/rcssmin/apidoc/epydoc.css
file path=usr/share/doc/rcssmin/apidoc/epydoc.js
file path=usr/share/doc/rcssmin/apidoc/help.html
file path=usr/share/doc/rcssmin/apidoc/identifier-index.html
file path=usr/share/doc/rcssmin/apidoc/index.html
file path=usr/share/doc/rcssmin/apidoc/module-tree.html
file path=usr/share/doc/rcssmin/apidoc/rcssmin-module.html
file path=usr/share/doc/rcssmin/apidoc/rcssmin-pysrc.html
file path=usr/share/doc/rcssmin/apidoc/redirect.html
file release-notes/legacy-library-python-rcssmin.txt \
    path=usr/share/doc/release-notes/legacy-library-python-rcssmin.txt \
    mode=0444 must-display=true release-note=feature/pkg/self@0
license rcssmin.license license="Apache v2.0"

# force the rename with an optional dependency on the old name
depend type=optional fmri=library/python/rcssmin@1.0.6-11.5.0.0.0.56.0
