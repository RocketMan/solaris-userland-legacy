set name=pkg.fmri \
    value=pkg:/legacy/library/python/jsonpatch-$(PYV)@$(IPS_COMPONENT_VERSION),$(BUILD_VERSION)
set name=pkg.summary \
    value="Python module for creating and applying JSON Patches"
set name=pkg.description \
    value="The jsonpatch Python module allows you to create and apply a JSON Patch -- a series of operations applied to a JSON document -- as defined by RFC 6902."
set name=com.oracle.info.description \
    value="the jsonpatch Python module for JSON Patch application"
set name=com.oracle.info.tpno value=$(TPNO)
set name=info.classification \
    value=org.opensolaris.category.2008:Development/Python
set name=info.source-url value=$(COMPONENT_ARCHIVE_URL)
set name=info.upstream value="Stefan KÃ¶gl"
set name=info.upstream-url value=$(COMPONENT_PROJECT_URL)
set name=org.opensolaris.arc-caseid value=PSARC/2013/151
set name=org.opensolaris.consolidation value=$(CONSOLIDATION)
#
file path=usr/lib/python$(PYVER)/vendor-packages/jsonpatch-$(COMPONENT_VERSION)-py$(PYVER).egg-info/PKG-INFO
file path=usr/lib/python$(PYVER)/vendor-packages/jsonpatch-$(COMPONENT_VERSION)-py$(PYVER).egg-info/SOURCES.txt
file path=usr/lib/python$(PYVER)/vendor-packages/jsonpatch-$(COMPONENT_VERSION)-py$(PYVER).egg-info/dependency_links.txt
file path=usr/lib/python$(PYVER)/vendor-packages/jsonpatch-$(COMPONENT_VERSION)-py$(PYVER).egg-info/requires.txt
file path=usr/lib/python$(PYVER)/vendor-packages/jsonpatch-$(COMPONENT_VERSION)-py$(PYVER).egg-info/top_level.txt
file path=usr/lib/python$(PYVER)/vendor-packages/jsonpatch.py
#
license jsonpatch.license license=BSD

# force the rename with an optional dependency on the old name
depend type=optional \
    fmri=library/python-2/jsonpatch-$(PYV)@1.1,5.12-5.12.0.0.0.41.0

# force the rename with an optional dependency on the old name
depend type=optional fmri=library/python/jsonpatch-$(PYV)@1.13-11.4.21.0.1.69.0

# force a dependency on the Python runtime
depend type=require fmri=__TBD pkg.debug.depend.file=python$(PYVER) \
    pkg.debug.depend.path=usr/bin

# force a dependency on the jsonpatch package
depend type=require \
    fmri=legacy/library/python/jsonpatch@$(IPS_COMPONENT_VERSION),$(BUILD_VERSION)

# force a dependency on jsonpointer; pkgdepend work is needed to flush this out.
depend type=require fmri=legacy/library/python/jsonpointer-$(PYV)
