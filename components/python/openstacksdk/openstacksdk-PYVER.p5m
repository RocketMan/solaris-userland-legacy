# force the rename with an optional dependency on the old name
depend type=optional \
    fmri=library/python/openstacksdk-$(PYV)@0.8.2-11.5.0.0.0.56.0

# force a dependency on the Python runtime
depend type=require fmri=__TBD pkg.debug.depend.file=python$(PYVER) \
    pkg.debug.depend.path=usr/bin

# force a dependency on the openstacksdk package
depend type=require \
    fmri=legacy/library/python/openstacksdk@$(IPS_COMPONENT_VERSION),$(BUILD_VERSION)

# force a dependency on os-client-config; pkgdepend work is needed to flush this
