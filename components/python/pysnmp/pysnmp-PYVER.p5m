# force a group dependency on the optional cryptography; pkgdepend work is
# needed to flush this out.
depend type=group fmri=library/python/cryptography-$(PYV)

# force the rename with an optional dependency on the old name
depend type=optional fmri=library/python/pysnmp-$(PYV)@4.3.2-11.5.0.0.0.56.0

# force a dependency on the Python runtime
depend type=require fmri=__TBD pkg.debug.depend.file=python$(PYVER) \
    pkg.debug.depend.path=usr/bin

# force a dependency on pysmi; pkgdepend work is needed to flush this out.
depend type=require fmri=legacy/library/python/pysmi-$(PYV)

# force a dependency on the pysnmp package
depend type=require \
    fmri=legacy/library/python/pysnmp@$(IPS_COMPONENT_VERSION),$(BUILD_VERSION)

# force a dependency on trollius; pkgdepend work is needed to flush this out.
depend type=require fmri=legacy/library/python/trollius-$(PYV)

# force a dependency on ipaddress; pkgdepend work is needed to flush this out.
depend type=require fmri=library/python/ipaddress-$(PYV)

# force a dependency on pyasn1; pkgdepend work is needed to flush this out.
depend type=require fmri=library/python/pyasn1-$(PYV)
